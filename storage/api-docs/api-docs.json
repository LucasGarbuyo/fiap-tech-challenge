                                            "cpf": {
                                                "type": "string",
                                                "description": "Preço do cliente."
                                            },
                                            "email": {
                                                "type": "string",
                                                "description": "Descrição do cliente."
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Nenhum cliente encontrado."
