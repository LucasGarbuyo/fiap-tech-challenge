{
    "openapi": "3.0.0",
    "info": {
        "title": "TechChallenge - API Documentation"
    },
    "servers": [
        {
            "url": "http://localhost:8000/api"
        }
    ],
    "paths": {
        "/product/index": {
            "get": {
                "tags": [
                    "Produtos"
                ],
                "summary": "Buscar produtos",
                "description": "Este endpoint é usado para buscar todos os produtos cadastrados no sistema.",
                "responses": {
                    "200": {
                        "description": "Listagem de produtos encontrada com sucesso.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "description": "ID único do produto."
                                            },
                                            "name": {
                                                "type": "string",
                                                "description": "Nome do produto."
                                            },
                                            "price": {
                                                "type": "number",
                                                "format": "float",
                                                "description": "Preço do produto."
                                            },
                                            "description": {
                                                "type": "string",
                                                "description": "Descrição do produto."
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Nenhum produto encontrado."
                    }
                }
            }
        },
        "/product/edit/{id}": {
            "get": {
                "tags": [
                    "Produtos"
                ],
                "summary": "Buscar produto por ID",
                "description": "Este endpoint é usado para buscar um produto específico pelo seu ID.",
                "parameters": [
                    {
                        "in": "path",
                        "name": "id",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "description": "ID único do produto."
                        },
                        "description": "ID do produto a ser buscado."
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Produto encontrado com sucesso.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "type": "string",
                                            "description": "ID único do produto."
                                        },
                                        "name": {
                                            "type": "string",
                                            "description": "Nome do produto."
                                        },
                                        "price": {
                                            "type": "number",
                                            "format": "float",
                                            "description": "Preço do produto."
                                        },
                                        "description": {
                                            "type": "string",
                                            "description": "Descrição do produto."
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Produto não encontrado."
                    }
                }
            }
        },
        "/product/store": {
            "post": {
                "tags": [
                    "Produtos"
                ],
                "summary": "Criar novo produto",
                "description": "Este endpoint é usado para criar um novo produto.",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "description": "Nome do produto.",
                                        "maxLength": 255
                                    },
                                    "description": {
                                        "type": "string",
                                        "description": "Descrição do produto."
                                    },
                                    "price": {
                                        "type": "number",
                                        "format": "float",
                                        "description": "Preço do produto."
                                    }
                                },
                                "required": [
                                    "name",
                                    "price"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Produto criado com sucesso."
                    },
                    "404": {
                        "description": "Nome do produto não pode ter mais que 255 caracteres."
                    }
                }
            }
        },
        "/product/update/{id}": {
            "put": {
                "tags": [
                    "Produtos"
                ],
                "summary": "Atualizar produto por ID",
                "description": "Este endpoint é usado para atualizar um produto existente pelo seu ID.",
                "parameters": [
                    {
                        "in": "path",
                        "name": "id",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "description": "ID único do produto."
                        },
                        "description": "ID do produto a ser atualizado."
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "description": "Nome do produto.",
                                        "maxLength": 255
                                    },
                                    "description": {
                                        "type": "string",
                                        "description": "Descrição do produto."
                                    },
                                    "price": {
                                        "type": "number",
                                        "format": "float",
                                        "description": "Preço do produto."
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Produto atualizado com sucesso."
                    },
                    "404": {
                        "description": "Produto não encontrado."
                    }
                }
            }
        },
        "/product/delete/{id}": {
            "delete": {
                "tags": [
                    "Produtos"
                ],
                "summary": "Deletar produto por ID",
                "description": "Este endpoint é usado para deletar um produto existente pelo seu ID. O produto deletado preencherá o campo com datetime o campo `deleted_at`.",
                "parameters": [
                    {
                        "in": "path",
                        "name": "id",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "description": "ID único do produto."
                        },
                        "description": "ID do produto a ser deletado."
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Produto deletado com sucesso."
                    },
                    "404": {
                        "description": "Produto não encontrado."
                    }
                }
            }
        },
